  <owl:Class rdf:about="http://www.github/argO/2016/9/argumentsontology#complexargument">
    <rdfs:subClassOf rdf:resource="http://www.github/argO/2016/9/argumentsontology/argument"/>
    <rdfs:subClassOf>
      <owl:Restriction rdf:nodeID="n84952ee2ef7347d9b6c94850498a5b0eb3">
        <owl:onProperty rdf:resource="http://www.github/argO/2016/9/argumentsontology#haspropercontinuantpart"/>
        <owl:someValuesFrom rdf:resource="http://www.github/argO/2016/9/argumentsontology/argument"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <test:test6c58c70d_1fcf_4414_be44_85e5cba8571b xml:lang="en">J. Neil Otte</test:test6c58c70d_1fcf_4414_be44_85e5cba8571b>
    <test:teste8bb6cb2_117b_4bfe_b5a6_a95b6576a498 xml:lang="en">An argument that has some argument as a proper continuant part, and thus also some subconclusion.</test:teste8bb6cb2_117b_4bfe_b5a6_a95b6576a498>
    <rdfs:label xml:lang="en">complex argument</rdfs:label>
  </owl:Class>
  <owl:Ontology rdf:about="http://www.github/argO/2016/9/argumentsontology">
    <owl:imports rdf:resource="http://purl.obolibrary.org/obo/iao/slim.owl."/>
    <owl:imports rdf:resource="http://www.ifomis.org/bfo/1.1"/>
    <test:testbe2221bc_d4e7_444b_8efe_5e4e837e77e7 xml:lang="en">ArgO: An Arguments Ontology is a small ontology created to facilitate the representation of arguments and their components: premises, conclusions, subconclusions, and suppositions.</test:testbe2221bc_d4e7_444b_8efe_5e4e837e77e7>
    <test:test0943e07c_ff8f_4dcc_bc0b_dbee849f4e0a xml:lang="en">Spring 2018</test:test0943e07c_ff8f_4dcc_bc0b_dbee849f4e0a>
    <owl:versionInfo xml:lang="en">Arguments Ontology (ArgO) v .3 -- development release</owl:versionInfo>
    <test:test4081ea94_ecbe_4908_94d3_7d6e0d99d226 xml:lang="en">J. Neil Otte, John Beverley, Francesco Franda, Yonatan Schrieber, Brian Donohue, and Alexander P. Cox.</test:test4081ea94_ecbe_4908_94d3_7d6e0d99d226>
  </owl:Ontology>
  <owl:ObjectProperty rdf:about="http://www.github/argO/2016/9/argumentsontology#creates">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/has_output">
        <owl:inverseOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/is_output_of"/>
        <test:teste8bb6cb2_117b_4bfe_b5a6_a95b6576a498 xml:lang="en">Inverse of is_output_of, a relation between a Processual Entity and a Continuant such that the presence of the Continuant at the end of the Processual Entity is a necessary condition for the completion of the Processual Entity (Source: Extended Relations Ontology / CCO)</test:teste8bb6cb2_117b_4bfe_b5a6_a95b6576a498>
        <rdfs:label xml:lang="en">has output</rdfs:label>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://www.github/argO/2016/9/argumentsontology#iscreatedby">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/is_output_of"/>
        <rdfs:domain rdf:resource="http://www.ifomis.org/bfo/1.1/snap#Continuant"/>
        <rdfs:range rdf:resource="http://www.ifomis.org/bfo/1.1/span#Process"/>
        <test:teste8bb6cb2_117b_4bfe_b5a6_a95b6576a498 xml:lang="en">X is created by Y iff X is output of Y and the terminal boundary of the temporal region (W) on which Y occurs interval_meets a terminal region (Z) at which X exists and X does not exist at W.</test:teste8bb6cb2_117b_4bfe_b5a6_a95b6576a498>
        <ns1:IAO_0000117 xml:lang="en">J. Neil Otte</ns1:IAO_0000117>
        <example xml:lang="en">An argument is created by some act of argument creation</example>
        <rdfs:label xml:lang="en">is created by</rdfs:label>
      </owl:ObjectProperty>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:Class rdf:about="http://www.github/argO/2016/9/argumentsontology/actofaffirming">
    <rdfs:subClassOf rdf:resource="http://www.github/argO/2016/9/argumentsontology/languageact"/>
    <rdfs:subClassOf>
      <owl:Restriction rdf:nodeID="n84952ee2ef7347d9b6c94850498a5b0eb5">
        <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/has_output"/>
        <owl:someValuesFrom rdf:resource="http://www.github/argO/2016/9/argumentsontology/sentencecontent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <test:test6c58c70d_1fcf_4414_be44_85e5cba8571b xml:lang="en">J. Neil Otte</test:test6c58c70d_1fcf_4414_be44_85e5cba8571b>
    <test:teste8bb6cb2_117b_4bfe_b5a6_a95b6576a498 xml:lang="en">a language act in which a sentence content is stated implicitly or explicitly in a forceful or confident manner, which conveys to others that the agent of the language act believes or has confidence in the truth or accuracy of the sentence content.</test:teste8bb6cb2_117b_4bfe_b5a6_a95b6576a498>
    <rdfs:label xml:lang="en">act of affirming</rdfs:label>
  </owl:Class>
  <owl:AnnotationProperty rdf:about="http://www.github/argO/2016/9/argumentsontology/example"/>
  <owl:Class rdf:about="http://www.github/argO/2016/9/argumentsontology#actofinferring">
    <rdfs:subClassOf rdf:resource="http://www.github/argO/2016/9/argumentsontology/languageact"/>
